<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Accordion maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="za.co.mahlaza.research.tocteditor.ui.controllers.CreateOrOpenTemplateController">
  <panes>
    <TitledPane animated="false" text="Create new template">
         <content>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextField layoutX="50.0" layoutY="76.0" promptText="Template name" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
                  <ChoiceBox layoutX="51.0" layoutY="134.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />

               </children>
            </AnchorPane>
         </content>
    </TitledPane>
    <TitledPane animated="false" text="Open existing template">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <Button id="chooseTemplateFileBtn" layoutX="218.0" layoutY="198.0" mnemonicParsing="false" text="Choose template file" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="200.0" onAction="#onChooseTemplateFile" />
               </children></AnchorPane>
      </content>
    </TitledPane>
  </panes>
</Accordion>
